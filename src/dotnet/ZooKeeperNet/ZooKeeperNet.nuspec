<?xml version="1.0"?>
<package >
  <metadata>
    <id>ZooKeeperNet</id>
    <version>$version$</version>
    <title>ZooKeeperNet</title>
    <authors>Eric Hauser, Chris Eldredge</authors>
    <owners>The Motley Fool, LLC</owners>
    <projectUrl>https://github.com/chriseldredge/zookeeper/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>ZooKeeper client for Microsoft.NET.</description>
    <copyright>Copyright 2015</copyright>
    <dependencies>
      <group targetFramework="net35">
        <dependency id="System.Threading.dll" version="1.0.2856"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$Configuration$\ZooKeeperNet.dll" target="lib\net35"/>
    <file src="bin\$Configuration$\ZooKeeperNet.pdb" target="lib\net35"/>
    <file src="bin\$Configuration$\ZooKeeperNet.xml" target="lib\net35"/>
    <file src="**\*.cs" target="src"/>
  </files>
</package>
